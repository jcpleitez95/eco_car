<div class="model-banner">
    <p class="brand-name"><%= @brand.name%> Models</p>
    <p class="click-info">Click for More Information</p>
</div>


<div class = "brand-model-container">
    <% @brand.models.each do |model| %>
        <div class="brand-model-tile">
        <%= link_to model.name, model_path(model)%>
        </div>
    <% end %>
</div>